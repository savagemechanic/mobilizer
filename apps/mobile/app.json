{
  "expo": {
    "name": "Mobilizer",
    "slug": "mobilizer",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mobilizer-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "scheme": "mobilizer",
    "splash": {
      "image": "./assets/images/mobilizer-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#3498DB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobilizer.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mobilizer-logo.png",
        "backgroundColor": "#3498DB"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mobilizer.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mobilizer-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#3498DB"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7b842fd8-5c5d-445f-be1b-182b0462b646"
      },
      "EXPO_PUBLIC_GRAPHQL_HTTP_URL": "https://mobilizer-backend-dyw1.onrender.com/graphql",
      "EXPO_PUBLIC_API_URL": "https://mobilizer-backend-dyw1.onrender.com",
      "EXPO_PUBLIC_ENV": "production"
    },
    "owner": "savagemechanic"
  }
}
